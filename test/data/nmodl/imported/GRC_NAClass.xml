<?xml version='1.0' encoding='UTF-8'?>
<nineml:NineML xmlns:nineml="http://nineml.net/9ML/1.0">
  <nineml:Dimension i="2" t="3" m="-1" l="-4" name="conductanceDensity"/>
  <nineml:ComponentClass name="GRC_NAClass">
    <nineml:AnalogSendPort dimension="currentDensity" name="ina"/>
    <nineml:AnalogReceivePort dimension="voltage" name="v"/>
    <nineml:Parameter dimension="voltage" name="ena"/>
    <nineml:Parameter dimension="dimensionless" name="ACoff"/>
    <nineml:Parameter dimension="dimensionless" name="AOoff"/>
    <nineml:Parameter dimension="dimensionless" name="Adelta"/>
    <nineml:Parameter dimension="dimensionless" name="Aepsilon"/>
    <nineml:Parameter dimension="dimensionless" name="AOon"/>
    <nineml:Parameter dimension="dimensionless" name="Aalfa"/>
    <nineml:Parameter dimension="conductanceDensity" name="gnabar"/>
    <nineml:Parameter dimension="dimensionless" name="ACon"/>
    <nineml:Parameter dimension="dimensionless" name="Vbeta"/>
    <nineml:Parameter dimension="dimensionless" name="Abeta"/>
    <nineml:Parameter dimension="temperature" name="celsius"/>
    <nineml:Parameter dimension="dimensionless" name="Ateta"/>
    <nineml:Parameter dimension="dimensionless" name="n1"/>
    <nineml:Parameter dimension="dimensionless" name="Vteta"/>
    <nineml:Parameter dimension="dimensionless" name="n3"/>
    <nineml:Parameter dimension="dimensionless" name="n4"/>
    <nineml:Parameter dimension="dimensionless" name="n2"/>
    <nineml:Parameter dimension="dimensionless" name="Agamma"/>
    <nineml:Parameter dimension="dimensionless" name="Valfa"/>
    <nineml:Dynamics>
      <nineml:Regime name="kstates">
        <nineml:TimeDerivative variable="I1">
          <nineml:MathInline>- (n1*alfa_v*a)*I1 + (n4*beta_v*b)*I2 - Coff*I1 + Con*C1</nineml:MathInline>
        </nineml:TimeDerivative>
        <nineml:TimeDerivative variable="I3">
          <nineml:MathInline>- (n3*beta_v*b)*I3 + (n2*alfa_v*a)*I2 - (n3*alfa_v*a)*I3 + (n2*beta_v*b)*I4 - (Coff*b^2)*I3 + (Con*a^2)*C3</nineml:MathInline>
        </nineml:TimeDerivative>
        <nineml:TimeDerivative variable="I2">
          <nineml:MathInline>- (n4*beta_v*b)*I2 + (n1*alfa_v*a)*I1 - (n2*alfa_v*a)*I2 + (n3*beta_v*b)*I3 - (Coff*b)*I2 + (Con*a)*C2</nineml:MathInline>
        </nineml:TimeDerivative>
        <nineml:TimeDerivative variable="I5">
          <nineml:MathInline>- (n1*beta_v*b)*I5 + (n4*alfa_v*a)*I4 - gamma*I5 + delta*I6 - (Coff*b^4)*I5 + (Con*a^4)*C5</nineml:MathInline>
        </nineml:TimeDerivative>
        <nineml:TimeDerivative variable="OB">
          <nineml:MathInline>- teta_v*OB + epsilon*O</nineml:MathInline>
        </nineml:TimeDerivative>
        <nineml:TimeDerivative variable="O">
          <nineml:MathInline>- delta*O + gamma*C5 - epsilon*O + teta_v*OB - Oon*O + Ooff*I6</nineml:MathInline>
        </nineml:TimeDerivative>
        <nineml:TimeDerivative variable="I4">
          <nineml:MathInline>- (n2*beta_v*b)*I4 + (n3*alfa_v*a)*I3 - (n4*alfa_v*a)*I4 + (n1*beta_v*b)*I5 - (Coff*b^3)*I4 + (Con*a^3)*C4</nineml:MathInline>
        </nineml:TimeDerivative>
        <nineml:TimeDerivative variable="C3">
          <nineml:MathInline>- (n3*beta_v)*C3 + (n2*alfa_v)*C2 - (n3*alfa_v)*C3 + (n2*beta_v)*C4 - (Con*a^2)*C3 + (Coff*b^2)*I3</nineml:MathInline>
        </nineml:TimeDerivative>
        <nineml:TimeDerivative variable="C2">
          <nineml:MathInline>- (n4*beta_v)*C2 + (n1*alfa_v)*C1 - (n2*alfa_v)*C2 + (n3*beta_v)*C3 - (Con*a)*C2 + (Coff*b)*I2</nineml:MathInline>
        </nineml:TimeDerivative>
        <nineml:TimeDerivative variable="C1">
          <nineml:MathInline>- (n1*alfa_v)*C1 + (n4*beta_v)*C2 - Con*C1 + Coff*I1</nineml:MathInline>
        </nineml:TimeDerivative>
        <nineml:TimeDerivative variable="I6">
          <nineml:MathInline>- delta*I6 + gamma*I5 - Ooff*I6 + Oon*O</nineml:MathInline>
        </nineml:TimeDerivative>
        <nineml:TimeDerivative variable="C5">
          <nineml:MathInline>- (n1*beta_v)*C5 + (n4*alfa_v)*C4 - gamma*C5 + delta*O - (Con*a^4)*C5 + (Coff*b^4)*I5</nineml:MathInline>
        </nineml:TimeDerivative>
        <nineml:TimeDerivative variable="C4">
          <nineml:MathInline>- (n2*beta_v)*C4 + (n3*alfa_v)*C3 - (n4*alfa_v)*C4 + (n1*beta_v)*C5 - (Con*a^3)*C4 + (Coff*b^3)*I4</nineml:MathInline>
        </nineml:TimeDerivative>
      </nineml:Regime>
      <nineml:Alias name="a">
        <nineml:MathInline>(Oon/Con)^0.25</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="Ooff">
        <nineml:MathInline>Q10 * AOoff</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="b">
        <nineml:MathInline>(Ooff/Coff)^0.25</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="g">
        <nineml:MathInline>gnabar * O</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="beta_v">
        <nineml:MathInline>Q10*Abeta*exp(-v/Vbeta)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="epsilon">
        <nineml:MathInline>Q10 * Aepsilon</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="Oon">
        <nineml:MathInline>Q10 * AOon</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="alfa_v">
        <nineml:MathInline>Q10*Aalfa*exp(v/Valfa)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="teta_v">
        <nineml:MathInline>Q10*Ateta*exp(-v/Vteta)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="Q10">
        <nineml:MathInline>3^((celsius-20)/10 )</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="delta">
        <nineml:MathInline>Q10 * Adelta</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="Coff">
        <nineml:MathInline>Q10 * ACoff</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="ina">
        <nineml:MathInline>g * (v - ena)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="gamma">
        <nineml:MathInline>Q10 * Agamma</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="Con">
        <nineml:MathInline>Q10 * ACon</nineml:MathInline>
      </nineml:Alias>
      <nineml:StateVariable dimension="dimensionless" name="I1"/>
      <nineml:StateVariable dimension="dimensionless" name="I3"/>
      <nineml:StateVariable dimension="dimensionless" name="I2"/>
      <nineml:StateVariable dimension="dimensionless" name="I5"/>
      <nineml:StateVariable dimension="dimensionless" name="OB"/>
      <nineml:StateVariable dimension="dimensionless" name="O"/>
      <nineml:StateVariable dimension="dimensionless" name="I4"/>
      <nineml:StateVariable dimension="dimensionless" name="C3"/>
      <nineml:StateVariable dimension="dimensionless" name="C2"/>
      <nineml:StateVariable dimension="dimensionless" name="C1"/>
      <nineml:StateVariable dimension="dimensionless" name="I6"/>
      <nineml:StateVariable dimension="dimensionless" name="C5"/>
      <nineml:StateVariable dimension="dimensionless" name="C4"/>
    </nineml:Dynamics>
  </nineml:ComponentClass>
  <nineml:Dimension i="-1" t="-3" m="1" l="2" name="voltage"/>
  <nineml:Dimension i="1" name="currentDensity" l="-2"/>
  <nineml:Dimension name="dimensionless"/>
  <nineml:Dimension k="1" name="temperature"/>
</nineml:NineML>
