<?xml version='1.0' encoding='UTF-8'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <ComponentClass name="GRC_NAClass">
    <AnalogSendPort dimension="currentDensity" name="ina"/>
    <AnalogReceivePort dimension="voltage" name="v"/>
    <Parameter dimension="voltage" name="ena"/>
    <Parameter name="ACoff"/>
    <Parameter name="AOoff"/>
    <Parameter name="Adelta"/>
    <Parameter name="Aepsilon"/>
    <Parameter name="AOon"/>
    <Parameter name="Aalfa"/>
    <Parameter dimension="conductanceDensity" name="gnabar"/>
    <Parameter name="ACon"/>
    <Parameter name="Vbeta"/>
    <Parameter name="Abeta"/>
    <Parameter dimension="temperature" name="celsius"/>
    <Parameter name="Ateta"/>
    <Parameter name="n1"/>
    <Parameter name="Vteta"/>
    <Parameter name="n3"/>
    <Parameter name="n4"/>
    <Parameter name="n2"/>
    <Parameter name="Agamma"/>
    <Parameter name="Valfa"/>
    <Dynamics>
      <Regime name="kstates">
        <TimeDerivative variable="I1">
          <MathInline>- (n1*alfa_v*a)*I1 + (n4*beta_v*b)*I2 - Coff*I1 + Con*C1</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="I3">
          <MathInline>- (n3*beta_v*b)*I3 + (n2*alfa_v*a)*I2 - (n3*alfa_v*a)*I3 + (n2*beta_v*b)*I4 - (Coff*b^2)*I3 + (Con*a^2)*C3</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="I2">
          <MathInline>- (n4*beta_v*b)*I2 + (n1*alfa_v*a)*I1 - (n2*alfa_v*a)*I2 + (n3*beta_v*b)*I3 - (Coff*b)*I2 + (Con*a)*C2</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="I5">
          <MathInline>- (n1*beta_v*b)*I5 + (n4*alfa_v*a)*I4 - gamma*I5 + delta*I6 - (Coff*b^4)*I5 + (Con*a^4)*C5</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="OB">
          <MathInline>- teta_v*OB + epsilon*O</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="O">
          <MathInline>- delta*O + gamma*C5 - epsilon*O + teta_v*OB - Oon*O + Ooff*I6</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="I4">
          <MathInline>- (n2*beta_v*b)*I4 + (n3*alfa_v*a)*I3 - (n4*alfa_v*a)*I4 + (n1*beta_v*b)*I5 - (Coff*b^3)*I4 + (Con*a^3)*C4</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="C3">
          <MathInline>- (n3*beta_v)*C3 + (n2*alfa_v)*C2 - (n3*alfa_v)*C3 + (n2*beta_v)*C4 - (Con*a^2)*C3 + (Coff*b^2)*I3</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="C2">
          <MathInline>- (n4*beta_v)*C2 + (n1*alfa_v)*C1 - (n2*alfa_v)*C2 + (n3*beta_v)*C3 - (Con*a)*C2 + (Coff*b)*I2</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="C1">
          <MathInline>- (n1*alfa_v)*C1 + (n4*beta_v)*C2 - Con*C1 + Coff*I1</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="I6">
          <MathInline>- delta*I6 + gamma*I5 - Ooff*I6 + Oon*O</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="C5">
          <MathInline>- (n1*beta_v)*C5 + (n4*alfa_v)*C4 - gamma*C5 + delta*O - (Con*a^4)*C5 + (Coff*b^4)*I5</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="C4">
          <MathInline>- (n2*beta_v)*C4 + (n3*alfa_v)*C3 - (n4*alfa_v)*C4 + (n1*beta_v)*C5 - (Con*a^3)*C4 + (Coff*b^3)*I4</MathInline>
        </TimeDerivative>
      </Regime>
      <Alias name="a">
        <MathInline>(Oon/Con)^0.25</MathInline>
      </Alias>
      <Alias name="Ooff">
        <MathInline>Q10 * AOoff</MathInline>
      </Alias>
      <Alias name="b">
        <MathInline>(Ooff/Coff)^0.25</MathInline>
      </Alias>
      <Alias name="g">
        <MathInline>gnabar * O</MathInline>
      </Alias>
      <Alias name="beta_v">
        <MathInline>Q10*Abeta*exp(-v/Vbeta)</MathInline>
      </Alias>
      <Alias name="epsilon">
        <MathInline>Q10 * Aepsilon</MathInline>
      </Alias>
      <Alias name="Oon">
        <MathInline>Q10 * AOon</MathInline>
      </Alias>
      <Alias name="alfa_v">
        <MathInline>Q10*Aalfa*exp(v/Valfa)</MathInline>
      </Alias>
      <Alias name="teta_v">
        <MathInline>Q10*Ateta*exp(-v/Vteta)</MathInline>
      </Alias>
      <Alias name="Q10">
        <MathInline>3^((celsius-20)/10 )</MathInline>
      </Alias>
      <Alias name="delta">
        <MathInline>Q10 * Adelta</MathInline>
      </Alias>
      <Alias name="Coff">
        <MathInline>Q10 * ACoff</MathInline>
      </Alias>
      <Alias name="ina">
        <MathInline>g * (v - ena)</MathInline>
      </Alias>
      <Alias name="gamma">
        <MathInline>Q10 * Agamma</MathInline>
      </Alias>
      <Alias name="Con">
        <MathInline>Q10 * ACon</MathInline>
      </Alias>
      <StateVariable name="I1"/>
      <StateVariable name="I3"/>
      <StateVariable name="I2"/>
      <StateVariable name="I5"/>
      <StateVariable name="OB"/>
      <StateVariable name="O"/>
      <StateVariable name="I4"/>
      <StateVariable name="C3"/>
      <StateVariable name="C2"/>
      <StateVariable name="C1"/>
      <StateVariable name="I6"/>
      <StateVariable name="C5"/>
      <StateVariable name="C4"/>
    </Dynamics>
  </ComponentClass>
  <nineml:Dimension xmlns:nineml="http://nineml.net/9ML/1.0" k="1" name="temperature"/>
  <nineml:Dimension xmlns:nineml="http://nineml.net/9ML/1.0" i="1" name="currentDensity" l="-2"/>
  <nineml:Dimension xmlns:nineml="http://nineml.net/9ML/1.0" i="2" t="3" m="-1" l="-4" name="conductanceDensity"/>
  <nineml:Dimension xmlns:nineml="http://nineml.net/9ML/1.0" i="-1" t="-3" m="1" l="2" name="voltage"/>
</NineML>
