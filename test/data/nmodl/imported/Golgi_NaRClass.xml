<?xml version='1.0' encoding='UTF-8'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <ComponentClass name="Golgi_NaRClass">
    <AnalogSendPort dimension="currentDensity" name="ina"/>
    <AnalogReceivePort dimension="voltage" name="ena"/>
    <AnalogReceivePort dimension="temperature" name="celsius"/>
    <AnalogReceivePort dimension="voltage" name="v"/>
    <Parameter dimension="per_time" name="Aalpha_f"/>
    <Parameter dimension="voltage" name="Kalpha_s"/>
    <Parameter dimension="per_time" name="Aalpha_s"/>
    <Parameter dimension="per_time" name="Abeta_s"/>
    <Parameter dimension="voltage" name="V0alpha_s"/>
    <Parameter dimension="per_time" name="Shiftalpha_s"/>
    <Parameter dimension="per_time" name="Abeta_f"/>
    <Parameter dimension="voltage" name="V0beta_f"/>
    <Parameter dimension="voltage" name="Kbeta_s"/>
    <Parameter dimension="conductanceDensity" name="gnabar"/>
    <Parameter dimension="per_time" name="Shiftbeta_s"/>
    <Parameter name="Q10"/>
    <Parameter dimension="voltage" name="Kalpha_f"/>
    <Parameter dimension="voltage" name="V0beta_s"/>
    <Parameter dimension="voltage" name="V0alpha_f"/>
    <Parameter dimension="voltage" name="Kbeta_f"/>
    <Dynamics>
      <Regime name="states">
        <TimeDerivative variable="s">
          <MathInline>( s_inf - s ) / tau_s</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="f">
          <MathInline>( f_inf - f ) / tau_f</MathInline>
        </TimeDerivative>
      </Regime>
      <Alias name="alpha_f">
        <MathInline>alp_f_v</MathInline>
      </Alias>
      <Alias name="beta_s">
        <MathInline>bet_s_v</MathInline>
      </Alias>
      <Alias name="bet_s_v">
        <MathInline>tcorr_v*(Shiftbeta_s+Abeta_s*((v+V0beta_s)/1  )/(exp(x1_v)-1))</MathInline>
      </Alias>
      <Alias name="a_s">
        <MathInline>alp_s_v</MathInline>
      </Alias>
      <Alias name="g">
        <MathInline>gnabar*s*f</MathInline>
      </Alias>
      <Alias name="tau_f">
        <MathInline>1 / ( a_f + b_f )</MathInline>
      </Alias>
      <Alias name="s_inf">
        <MathInline>a_s / ( a_s + b_s )</MathInline>
      </Alias>
      <Alias name="alp_s_v">
        <MathInline>tcorr_v*(Shiftalpha_s+Aalpha_s*((v+V0alpha_s)/ 1  )/(exp((v+V0alpha_s)/Kalpha_s)-1))</MathInline>
      </Alias>
      <Alias name="tau_s">
        <MathInline>1 / ( a_s + b_s )</MathInline>
      </Alias>
      <Alias name="x1_v">
        <Piecewise>
          <Piece>
            <MathInline>200</MathInline>
            <MathInline>x1_v&gt;200</MathInline>
          </Piece>
          <Otherwise>
            <MathInline>(v+V0beta_s)/Kbeta_s</MathInline>
          </Otherwise>
        </Piecewise>
      </Alias>
      <Alias name="f_inf">
        <MathInline>a_f / ( a_f + b_f )</MathInline>
      </Alias>
      <Alias name="a_f">
        <MathInline>alp_f_v</MathInline>
      </Alias>
      <Alias name="b_f">
        <MathInline>bet_f_v</MathInline>
      </Alias>
      <Alias name="tcorr_v">
        <MathInline>Q10^( ( celsius - 20  ) / 10  )</MathInline>
      </Alias>
      <Alias name="beta_f">
        <MathInline>bet_f_v</MathInline>
      </Alias>
      <Alias name="alp_f_v">
        <MathInline>tcorr_v * Aalpha_f * exp( ( v - V0alpha_f ) / Kalpha_f)</MathInline>
      </Alias>
      <Alias name="b_s">
        <MathInline>bet_s_v</MathInline>
      </Alias>
      <Alias name="ina">
        <MathInline>g*(v - ena)</MathInline>
      </Alias>
      <Alias name="bet_f_v">
        <MathInline>tcorr_v * Abeta_f * exp( ( v - V0beta_f ) / Kbeta_f )</MathInline>
      </Alias>
      <Alias name="alpha_s">
        <MathInline>alp_s_v</MathInline>
      </Alias>
      <StateVariable name="s"/>
      <StateVariable name="f"/>
    </Dynamics>
  </ComponentClass>
  <nineml:Dimension xmlns:nineml="http://nineml.net/9ML/1.0" name="per_time" t="-1"/>
  <nineml:Dimension xmlns:nineml="http://nineml.net/9ML/1.0" i="-1" t="-3" m="1" l="2" name="voltage"/>
  <nineml:Dimension xmlns:nineml="http://nineml.net/9ML/1.0" k="1" name="temperature"/>
  <nineml:Dimension xmlns:nineml="http://nineml.net/9ML/1.0" i="2" t="3" m="-1" l="-4" name="conductanceDensity"/>
  <nineml:Dimension xmlns:nineml="http://nineml.net/9ML/1.0" i="1" name="currentDensity" l="-2"/>
</NineML>
