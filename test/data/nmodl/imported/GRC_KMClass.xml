<?xml version='1.0' encoding='UTF-8'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <ComponentClass name="GRC_KMClass">
    <AnalogSendPort dimension="currentDensity" name="ik"/>
    <AnalogReceivePort dimension="voltage" name="v"/>
    <Parameter dimension="voltage" name="V0_ninf"/>
    <Parameter dimension="voltage" name="ek"/>
    <Parameter dimension="voltage" name="V0beta_n"/>
    <Parameter dimension="conductanceDensity" name="gkbar"/>
    <Parameter dimension="voltage" name="Kbeta_n"/>
    <Parameter dimension="temperature" name="celsius"/>
    <Parameter dimension="voltage" name="V0alpha_n"/>
    <Parameter dimension="per_time" name="Aalpha_n"/>
    <Parameter dimension="voltage" name="B_ninf"/>
    <Parameter dimension="per_time" name="Abeta_n"/>
    <Parameter dimension="voltage" name="Kalpha_n"/>
    <Dynamics>
      <Regime name="states">
        <TimeDerivative variable="n">
          <MathInline>(n_inf - n)/tau_n</MathInline>
        </TimeDerivative>
      </Regime>
      <Alias name="tau_n">
        <MathInline>1/(a_n + b_n)</MathInline>
      </Alias>
      <Alias name="b_n">
        <MathInline>bet_n_v</MathInline>
      </Alias>
      <Alias name="Q10_v">
        <MathInline>3^((celsius-22)/10)</MathInline>
      </Alias>
      <Alias name="bet_n_v">
        <MathInline>Q10_v*Abeta_n*exp((v-V0beta_n)/Kbeta_n)</MathInline>
      </Alias>
      <Alias name="alp_n_v">
        <MathInline>Q10_v*Aalpha_n*exp((v-V0alpha_n)/Kalpha_n)</MathInline>
      </Alias>
      <Alias name="n_inf">
        <MathInline>1/(1+exp(-(v-V0_ninf)/B_ninf))</MathInline>
      </Alias>
      <Alias name="alpha_n">
        <MathInline>alp_n_v</MathInline>
      </Alias>
      <Alias name="g">
        <MathInline>gkbar*n</MathInline>
      </Alias>
      <Alias name="ik">
        <MathInline>g*(v - ek)</MathInline>
      </Alias>
      <Alias name="a_n">
        <MathInline>alp_n_v</MathInline>
      </Alias>
      <Alias name="beta_n">
        <MathInline>bet_n_v</MathInline>
      </Alias>
      <StateVariable name="n"/>
    </Dynamics>
  </ComponentClass>
  <nineml:Dimension xmlns:nineml="http://nineml.net/9ML/1.0" name="per_time" t="-1"/>
  <nineml:Dimension xmlns:nineml="http://nineml.net/9ML/1.0" i="-1" t="-3" m="1" l="2" name="voltage"/>
  <nineml:Dimension xmlns:nineml="http://nineml.net/9ML/1.0" k="1" name="temperature"/>
  <nineml:Dimension xmlns:nineml="http://nineml.net/9ML/1.0" i="2" t="3" m="-1" l="-4" name="conductanceDensity"/>
  <nineml:Dimension xmlns:nineml="http://nineml.net/9ML/1.0" i="1" name="currentDensity" l="-2"/>
</NineML>
