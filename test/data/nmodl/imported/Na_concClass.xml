<?xml version='1.0' encoding='UTF-8'?>
<nineml:NineML xmlns:nineml="http://nineml.net/9ML/1.0">
  <nineml:Unit symbol="C" dimension="charge" power="0"/>
  <nineml:Dimension name="per_time" t="-1"/>
  <nineml:Dimension i="1" name="currentDensity" l="-2"/>
  <nineml:Dimension i="1" name="charge" t="1"/>
  <nineml:Dimension name="length" l="1"/>
  <nineml:ComponentClass name="na_concClass">
    <nineml:AnalogSendPort dimension="concentration" name="nai"/>
    <nineml:AnalogSendPort dimension="concentration" name="nao"/>
    <nineml:AnalogReceivePort dimension="currentDensity" name="ina"/>
    <nineml:Parameter dimension="per_time" name="beta"/>
    <nineml:Parameter dimension="concentration" name="nai0"/>
    <nineml:Parameter dimension="length" name="d"/>
    <nineml:Parameter dimension="concentration" name="nao0"/>
    <nineml:Dynamics>
      <nineml:Regime name="conc">
        <nineml:TimeDerivative variable="nai">
          <nineml:MathInline>-(ina)/(2*FARADAY*d)*(1e4) - beta*(nai-nai0)</nineml:MathInline>
        </nineml:TimeDerivative>
        <nineml:TimeDerivative variable="nao">
          <nineml:MathInline>(ina)/(2*FARADAY*d)*(1e4) - beta*(nao-nao0)</nineml:MathInline>
        </nineml:TimeDerivative>
      </nineml:Regime>
      <nineml:Constant units="C" name="FARADAY">96485.3365</nineml:Constant>
      <nineml:StateVariable dimension="dimensionless" name="nai"/>
      <nineml:StateVariable dimension="dimensionless" name="nao"/>
    </nineml:Dynamics>
  </nineml:ComponentClass>
  <nineml:Dimension name="dimensionless"/>
  <nineml:Dimension l="-3" name="concentration" n="1"/>
</nineml:NineML>
