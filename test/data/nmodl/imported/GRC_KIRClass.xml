<?xml version='1.0' encoding='UTF-8'?>
<nineml:NineML xmlns:nineml="http://nineml.net/9ML/1.0">
  <nineml:Dimension name="per_time" t="-1"/>
  <nineml:Dimension i="2" t="3" m="-1" l="-4" name="conductanceDensity"/>
  <nineml:Dimension i="1" name="currentDensity" l="-2"/>
  <nineml:Dimension i="-1" t="-3" m="1" l="2" name="voltage"/>
  <nineml:ComponentClass name="GRC_KIRClass">
    <nineml:AnalogSendPort dimension="currentDensity" name="ik"/>
    <nineml:AnalogReceivePort dimension="voltage" name="v"/>
    <nineml:Parameter dimension="voltage" name="ek"/>
    <nineml:Parameter dimension="voltage" name="Kbeta_d"/>
    <nineml:Parameter dimension="conductanceDensity" name="gkbar"/>
    <nineml:Parameter dimension="voltage" name="V0beta_d"/>
    <nineml:Parameter dimension="per_time" name="Abeta_d"/>
    <nineml:Parameter dimension="per_time" name="Aalpha_d"/>
    <nineml:Parameter dimension="temperature" name="celsius"/>
    <nineml:Parameter dimension="voltage" name="Kalpha_d"/>
    <nineml:Parameter dimension="voltage" name="V0alpha_d"/>
    <nineml:Dynamics>
      <nineml:Regime name="states">
        <nineml:TimeDerivative variable="d">
          <nineml:MathInline>(d_inf - d)/tau_d</nineml:MathInline>
        </nineml:TimeDerivative>
      </nineml:Regime>
      <nineml:Alias name="bet_d_v">
        <nineml:MathInline>Q10_v*Abeta_d*exp((v-V0beta_d)/Kbeta_d)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="alpha_d">
        <nineml:MathInline>alp_d_v</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="Q10_v">
        <nineml:MathInline>3^((celsius-20)/10)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="g">
        <nineml:MathInline>gkbar*d</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="tau_d">
        <nineml:MathInline>1/(a_d + b_d)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="b_d">
        <nineml:MathInline>bet_d_v</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="alp_d_v">
        <nineml:MathInline>Q10_v*Aalpha_d*exp((v-V0alpha_d)/Kalpha_d)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="ik">
        <nineml:MathInline>g*(v - ek)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="a_d">
        <nineml:MathInline>alp_d_v</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="beta_d">
        <nineml:MathInline>bet_d_v</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="d_inf">
        <nineml:MathInline>a_d/(a_d + b_d)</nineml:MathInline>
      </nineml:Alias>
      <nineml:StateVariable dimension="dimensionless" name="d"/>
    </nineml:Dynamics>
  </nineml:ComponentClass>
  <nineml:Dimension name="dimensionless"/>
  <nineml:Dimension k="1" name="temperature"/>
</nineml:NineML>
