<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <Dynamics name="Golgi_hcn1Class">
    <AnalogSendPort dimension="currentDensity" name="ih"/>
    <AnalogReceivePort dimension="temperature" name="celsius"/>
    <AnalogReceivePort dimension="voltage" name="v"/>
    <Parameter dimension="per_voltage" name="c"/>
    <Parameter dimension="voltage" name="Erev"/>
    <Parameter dimension="dimensionless" name="q_10"/>
    <Parameter dimension="voltage" name="tDf"/>
    <Parameter dimension="per_voltage" name="tEf"/>
    <Parameter dimension="dimensionless" name="tCs"/>
    <Parameter dimension="voltage" name="tDs"/>
    <Parameter dimension="voltage" name="Ehalf"/>
    <Parameter dimension="per_voltage" name="rA"/>
    <Parameter dimension="dimensionless" name="rB"/>
    <Parameter dimension="dimensionless" name="tCf"/>
    <Parameter dimension="conductanceDensity" name="gbar"/>
    <Parameter dimension="per_voltage" name="tEs"/>
    
      <Regime name="state">
        <TimeDerivative variable="o_fast">
          <MathInline>(o_fast_inf - o_fast) / tau_f</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="o_slow">
          <MathInline>(o_slow_inf - o_slow) / tau_s</MathInline>
        </TimeDerivative>
      </Regime>
      <Alias name="o_slow_inf">
        <MathInline>(1 - r_v) * o_inf_v_Ehalf_c</MathInline>
      </Alias>
      <Alias name="tau_v_tCs_tDs_tEs">
        <MathInline>exp(((tCs * v) - tDs)*tEs)</MathInline>
      </Alias>
      <Alias name="o_fast_inf">
        <MathInline>r_v * o_inf_v_Ehalf_c</MathInline>
      </Alias>
      <Alias name="o_inf_v_Ehalf_c">
        <MathInline>1 / (1 + exp((v - Ehalf) * c))</MathInline>
      </Alias>
      <Alias name="tau_v_tCf_tDf_tEf">
        <MathInline>exp(((tCf * v) - tDf)*tEf)</MathInline>
      </Alias>
      <Alias name="tau_f">
        <MathInline>tau_v_tCf_tDf_tEf</MathInline>
      </Alias>
      <Alias name="r_v">
        <MathInline>rA * v + rB</MathInline>
      </Alias>
      <Alias name="q">
        <MathInline>q_10^((celsius -33) / 10)</MathInline>
      </Alias>
      <Alias name="ih">
        <MathInline>g * (v - Erev)</MathInline>
      </Alias>
      <Alias name="g">
        <MathInline>gbar * (o_fast + o_slow)</MathInline>
      </Alias>
      <Alias name="tau_s">
        <MathInline>tau_v_tCs_tDs_tEs</MathInline>
      </Alias>
      <StateVariable dimension="dimensionless" name="o_fast"/>
      <StateVariable dimension="dimensionless" name="o_slow"/>
    </Dynamics>
  <Dimension i="2" t="3" m="-1" l="-4" name="conductanceDensity"/>
  <Dimension i="1" t="3" m="-1" l="-2" name="per_voltage"/>
  <Dimension i="-1" t="-3" m="1" l="2" name="voltage"/>
  <Dimension i="1" name="currentDensity" l="-2"/>
  <Dimension name="dimensionless"/>
  <Dimension k="1" name="temperature"/>
</NineML>
