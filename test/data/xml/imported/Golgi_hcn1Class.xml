<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<nineml:NineML xmlns:nineml="http://nineml.net/9ML/1.0">
  <nineml:ComponentClass name="Golgi_hcn1Class">
    <nineml:AnalogSendPort dimension="currentDensity" name="ih"/>
    <nineml:AnalogReceivePort dimension="temperature" name="celsius"/>
    <nineml:AnalogReceivePort dimension="voltage" name="v"/>
    <nineml:Parameter dimension="per_voltage" name="c"/>
    <nineml:Parameter dimension="voltage" name="Erev"/>
    <nineml:Parameter dimension="dimensionless" name="q_10"/>
    <nineml:Parameter dimension="voltage" name="tDf"/>
    <nineml:Parameter dimension="per_voltage" name="tEf"/>
    <nineml:Parameter dimension="dimensionless" name="tCs"/>
    <nineml:Parameter dimension="voltage" name="tDs"/>
    <nineml:Parameter dimension="voltage" name="Ehalf"/>
    <nineml:Parameter dimension="per_voltage" name="rA"/>
    <nineml:Parameter dimension="dimensionless" name="rB"/>
    <nineml:Parameter dimension="dimensionless" name="tCf"/>
    <nineml:Parameter dimension="conductanceDensity" name="gbar"/>
    <nineml:Parameter dimension="per_voltage" name="tEs"/>
    <nineml:Dynamics>
      <nineml:Regime name="state">
        <nineml:TimeDerivative variable="o_fast">
          <nineml:MathInline>(o_fast_inf - o_fast) / tau_f</nineml:MathInline>
        </nineml:TimeDerivative>
        <nineml:TimeDerivative variable="o_slow">
          <nineml:MathInline>(o_slow_inf - o_slow) / tau_s</nineml:MathInline>
        </nineml:TimeDerivative>
      </nineml:Regime>
      <nineml:Alias name="o_slow_inf">
        <nineml:MathInline>(1 - r_v) * o_inf_v_Ehalf_c</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="tau_v_tCs_tDs_tEs">
        <nineml:MathInline>exp(((tCs * v) - tDs)*tEs)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="o_fast_inf">
        <nineml:MathInline>r_v * o_inf_v_Ehalf_c</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="o_inf_v_Ehalf_c">
        <nineml:MathInline>1 / (1 + exp((v - Ehalf) * c))</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="tau_v_tCf_tDf_tEf">
        <nineml:MathInline>exp(((tCf * v) - tDf)*tEf)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="tau_f">
        <nineml:MathInline>tau_v_tCf_tDf_tEf</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="r_v">
        <nineml:MathInline>rA * v + rB</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="q">
        <nineml:MathInline>q_10^((celsius -33) / 10)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="ih">
        <nineml:MathInline>g * (v - Erev)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="g">
        <nineml:MathInline>gbar * (o_fast + o_slow)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="tau_s">
        <nineml:MathInline>tau_v_tCs_tDs_tEs</nineml:MathInline>
      </nineml:Alias>
      <nineml:StateVariable dimension="dimensionless" name="o_fast"/>
      <nineml:StateVariable dimension="dimensionless" name="o_slow"/>
    </nineml:Dynamics>
  </nineml:ComponentClass>
  <nineml:Dimension i="2" t="3" m="-1" l="-4" name="conductanceDensity"/>
  <nineml:Dimension i="1" t="3" m="-1" l="-2" name="per_voltage"/>
  <nineml:Dimension i="-1" t="-3" m="1" l="2" name="voltage"/>
  <nineml:Dimension i="1" name="currentDensity" l="-2"/>
  <nineml:Dimension name="dimensionless"/>
  <nineml:Dimension k="1" name="temperature"/>
</nineml:NineML>
