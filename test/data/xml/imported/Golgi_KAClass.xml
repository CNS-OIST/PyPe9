<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <Dimension name="per_time" t="-1"/>
  <Dimension i="2" t="3" m="-1" l="-4" name="conductanceDensity"/>
  <Dynamics name="Golgi_KAClass">
    <AnalogSendPort dimension="currentDensity" name="ik"/>
    <AnalogReceivePort dimension="temperature" name="celsius"/>
    <AnalogReceivePort dimension="voltage" name="ek"/>
    <AnalogReceivePort dimension="voltage" name="v"/>
    <Parameter dimension="voltage" name="Kbeta_b"/>
    <Parameter dimension="voltage" name="Kbeta_a"/>
    <Parameter dimension="per_time" name="Abeta_b"/>
    <Parameter dimension="voltage" name="K_binf"/>
    <Parameter dimension="voltage" name="V0beta_a"/>
    <Parameter dimension="voltage" name="V0beta_b"/>
    <Parameter dimension="conductanceDensity" name="gkbar"/>
    <Parameter dimension="voltage" name="K_ainf"/>
    <Parameter dimension="voltage" name="V0_ainf"/>
    <Parameter dimension="voltage" name="Kalpha_a"/>
    <Parameter dimension="voltage" name="Kalpha_b"/>
    <Parameter dimension="per_time" name="Abeta_a"/>
    <Parameter dimension="dimensionless" name="Q10"/>
    <Parameter dimension="per_time" name="Aalpha_a"/>
    <Parameter dimension="voltage" name="V0alpha_b"/>
    <Parameter dimension="per_time" name="Aalpha_b"/>
    <Parameter dimension="voltage" name="V0_binf"/>
    <Parameter dimension="voltage" name="V0alpha_a"/>
    
      <Regime name="states">
        <TimeDerivative variable="a">
          <MathInline>(a_inf - a)/tau_a</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="b">
          <MathInline>(b_inf - b)/tau_b</MathInline>
        </TimeDerivative>
      </Regime>
      <Alias name="a_inf">
        <MathInline>1/(1+exp((v-V0_ainf)/K_ainf))</MathInline>
      </Alias>
      <Alias name="ik">
        <MathInline>g*(v - ek)</MathInline>
      </Alias>
      <Alias name="b_inf">
        <MathInline>1/(1+exp((v-V0_binf)/K_binf))</MathInline>
      </Alias>
      <Alias name="tcorr_v">
        <MathInline>Q10^((celsius-25.5)/10)</MathInline>
      </Alias>
      <Alias name="tau_b">
        <MathInline>1/(a_b + b_b)</MathInline>
      </Alias>
      <Alias name="tau_a">
        <MathInline>1/(a_a + b_a)</MathInline>
      </Alias>
      <Alias name="beta_a">
        <MathInline>bet_a_v</MathInline>
      </Alias>
      <Alias name="beta_b">
        <MathInline>bet_b_v</MathInline>
      </Alias>
      <Alias name="bet_b_v">
        <MathInline>tcorr_v*Abeta_b*sigm_v__m__V0beta_b_Kbeta_b</MathInline>
      </Alias>
      <Alias name="alpha_b">
        <MathInline>alp_b_v</MathInline>
      </Alias>
      <Alias name="alpha_a">
        <MathInline>alp_a_v</MathInline>
      </Alias>
      <Alias name="b_a">
        <MathInline>bet_a_v</MathInline>
      </Alias>
      <Alias name="b_b">
        <MathInline>bet_b_v</MathInline>
      </Alias>
      <Alias name="bet_a_v">
        <MathInline>tcorr_v*Abeta_a/(exp((v-V0beta_a)/Kbeta_a))</MathInline>
      </Alias>
      <Alias name="sigm_v__m__V0alpha_a_Kalpha_a">
        <MathInline>1/(exp((v-V0alpha_a)/Kalpha_a) + 1)</MathInline>
      </Alias>
      <Alias name="a_b">
        <MathInline>alp_b_v</MathInline>
      </Alias>
      <Alias name="a_a">
        <MathInline>alp_a_v</MathInline>
      </Alias>
      <Alias name="sigm_v__m__V0beta_b_Kbeta_b">
        <MathInline>1/(exp((v-V0beta_b)/Kbeta_b) + 1)</MathInline>
      </Alias>
      <Alias name="alp_b_v">
        <MathInline>tcorr_v*Aalpha_b*sigm_v__m__V0alpha_b_Kalpha_b</MathInline>
      </Alias>
      <Alias name="g">
        <MathInline>gkbar*a*a*a*b</MathInline>
      </Alias>
      <Alias name="alp_a_v">
        <MathInline>tcorr_v*Aalpha_a*sigm_v__m__V0alpha_a_Kalpha_a</MathInline>
      </Alias>
      <Alias name="sigm_v__m__V0alpha_b_Kalpha_b">
        <MathInline>1/(exp((v-V0alpha_b)/Kalpha_b) + 1)</MathInline>
      </Alias>
      <StateVariable dimension="dimensionless" name="a"/>
      <StateVariable dimension="dimensionless" name="b"/>
    </Dynamics>
  <Dimension i="-1" t="-3" m="1" l="2" name="voltage"/>
  <Dimension i="1" name="currentDensity" l="-2"/>
  <Dimension name="dimensionless"/>
  <Dimension k="1" name="temperature"/>
</NineML>
