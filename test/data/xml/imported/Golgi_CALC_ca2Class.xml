<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<nineml:NineML xmlns:nineml="http://nineml.net/9ML/1.0">
  <nineml:Unit symbol="C" dimension="charge" power="0"/>
  <nineml:Dimension name="per_time" t="-1"/>
  <nineml:Dimension i="1" name="charge" t="1"/>
  <nineml:ComponentClass name="Golgi_CALC_ca2Class">
    <nineml:AnalogSendPort dimension="concentration" name="ca2i"/>
    <nineml:AnalogReceivePort dimension="currentDensity" name="ica2"/>
    <nineml:AnalogReceivePort dimension="concentration" name="ca2o"/>
    <nineml:Parameter dimension="concentration" name="ca2i0"/>
    <nineml:Parameter dimension="per_time" name="beta"/>
    <nineml:Parameter dimension="length" name="d"/>
    <nineml:Dynamics>
      <nineml:Regime name="conc">
        <nineml:TimeDerivative variable="ca2i">
          <nineml:MathInline>-ica2/(2*F*d)*(1e4) - beta*(ca2i-ca2i0)</nineml:MathInline>
        </nineml:TimeDerivative>
      </nineml:Regime>
      <nineml:Alias name="ca2_pump_i">
        <nineml:MathInline>2*beta*(ca2i-ca2i0)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Constant units="C" name="F">96485.3365</nineml:Constant>
      <nineml:StateVariable dimension="dimensionless" name="ca2i"/>
    </nineml:Dynamics>
  </nineml:ComponentClass>
  <nineml:Dimension name="length" l="1"/>
  <nineml:Dimension i="1" name="currentDensity" l="-2"/>
  <nineml:Dimension name="dimensionless"/>
  <nineml:Dimension l="-3" name="concentration" n="1"/>
</nineml:NineML>
