<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <Unit symbol="C" dimension="charge" power="0"/>
  <Dimension name="per_time" t="-1"/>
  <Dimension i="1" name="charge" t="1"/>
  <Dynamics name="k_concClass">
    <AnalogSendPort dimension="concentration" name="ki"/>
    <AnalogSendPort dimension="concentration" name="ko"/>
    <AnalogReceivePort dimension="currentDensity" name="ik"/>
    <Parameter dimension="concentration" name="ko0"/>
    <Parameter dimension="per_time" name="beta"/>
    <Parameter dimension="concentration" name="ki0"/>
    <Parameter dimension="length" name="d"/>
    
      <Regime name="conc">
        <TimeDerivative variable="ki">
          <MathInline>-(ik)/(2*FARADAY*d)*(1e4) - beta*(ki-ki0)</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="ko">
          <MathInline>(ik)/(2*FARADAY*d)*(1e4) - beta*(ko-ko0)</MathInline>
        </TimeDerivative>
      </Regime>
      <Constant units="C" name="FARADAY" value="96485.3365"/>
      <StateVariable dimension="dimensionless" name="ki"/>
      <StateVariable dimension="dimensionless" name="ko"/>
    </Dynamics>
  <Dimension name="length" l="1"/>
  <Dimension i="1" name="currentDensity" l="-2"/>
  <Dimension name="dimensionless"/>
  <Dimension l="-3" name="concentration" n="1"/>
</NineML>
