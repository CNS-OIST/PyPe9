<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <Dimension i="2" t="3" m="-1" l="-4" name="conductanceDensity"/>
  <Dimension i="-1" t="-3" m="1" l="2" name="voltage"/>
  <Dimension i="1" name="currentDensity" l="-2"/>
  <Dimension name="dimensionless"/>
  <Dynamics name="Golgi_SK2Class">
    <AnalogSendPort dimension="currentDensity" name="ik"/>
    <AnalogReceivePort dimension="concentration" name="cai"/>
    <AnalogReceivePort dimension="temperature" name="celsius"/>
    <AnalogReceivePort dimension="voltage" name="ek"/>
    <AnalogReceivePort dimension="voltage" name="v"/>
    <Parameter dimension="dimensionless" name="dirc2"/>
    <Parameter dimension="dimensionless" name="invo1"/>
    <Parameter dimension="dimensionless" name="diro1"/>
    <Parameter dimension="dimensionless" name="dirc3"/>
    <Parameter dimension="dimensionless" name="invo2"/>
    <Parameter dimension="conductanceDensity" name="gkbar"/>
    <Parameter dimension="dimensionless" name="dirc4"/>
    <Parameter dimension="dimensionless" name="Q10"/>
    <Parameter dimension="dimensionless" name="diff"/>
    <Parameter dimension="dimensionless" name="diro2"/>
    <Parameter dimension="dimensionless" name="invc1"/>
    <Parameter dimension="dimensionless" name="invc3"/>
    <Parameter dimension="dimensionless" name="invc2"/>
    
      <Regime name="kin">
        <TimeDerivative variable="c3">
          <MathInline>- invc2_t*c3 + dirc3_t_ca*c2 - dirc4_t_ca*c3 + invc3_t*c4 - diro1_t*c3 + invo1_t*o1</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c2">
          <MathInline>- invc1_t*c2 + dirc2_t_ca*c1 - dirc3_t_ca*c2 + invc2_t*c3</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c1">
          <MathInline>- dirc2_t_ca*c1 + invc1_t*c2</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="o2">
          <MathInline>- invo2_t*o2 + diro2_t*c4</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="o1">
          <MathInline>- invo1_t*o1 + diro1_t*c3</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="c4">
          <MathInline>- invc3_t*c4 + dirc4_t_ca*c3 - diro2_t*c4 + invo2_t*o2</MathInline>
        </TimeDerivative>
      </Regime>
      <Alias name="invc2_t">
        <MathInline>invc2*tcorr</MathInline>
      </Alias>
      <Alias name="diro2_t">
        <MathInline>diro2*tcorr</MathInline>
      </Alias>
      <Alias name="invo1_t">
        <MathInline>invo1*tcorr</MathInline>
      </Alias>
      <Alias name="dirc3_t">
        <MathInline>dirc3*tcorr</MathInline>
      </Alias>
      <Alias name="dirc4_t">
        <MathInline>dirc4*tcorr</MathInline>
      </Alias>
      <Alias name="invo2_t">
        <MathInline>invo2*tcorr</MathInline>
      </Alias>
      <Alias name="dirc2_t">
        <MathInline>dirc2*tcorr</MathInline>
      </Alias>
      <Alias name="dirc4_t_ca">
        <MathInline>dirc4_t*(cai/diff)</MathInline>
      </Alias>
      <Alias name="invc1_t">
        <MathInline>invc1*tcorr</MathInline>
      </Alias>
      <Alias name="g">
        <MathInline>gkbar*(o1+o2)</MathInline>
      </Alias>
      <Alias name="ik">
        <MathInline>g*(v-ek)</MathInline>
      </Alias>
      <Alias name="dirc3_t_ca">
        <MathInline>dirc3_t*(cai/diff)</MathInline>
      </Alias>
      <Alias name="diro1_t">
        <MathInline>diro1*tcorr</MathInline>
      </Alias>
      <Alias name="invc3_t">
        <MathInline>invc3*tcorr</MathInline>
      </Alias>
      <Alias name="temper_Q10_celsius">
        <MathInline>Q10^((celsius -23) / 10)</MathInline>
      </Alias>
      <Alias name="dirc2_t_ca">
        <MathInline>dirc2_t*(cai/diff)</MathInline>
      </Alias>
      <Alias name="tcorr">
        <MathInline>temper_Q10_celsius</MathInline>
      </Alias>
      <StateVariable dimension="dimensionless" name="c3"/>
      <StateVariable dimension="dimensionless" name="c2"/>
      <StateVariable dimension="dimensionless" name="c1"/>
      <StateVariable dimension="dimensionless" name="o2"/>
      <StateVariable dimension="dimensionless" name="o1"/>
      <StateVariable dimension="dimensionless" name="c4"/>
    </Dynamics>
  <Dimension l="-3" name="concentration" n="1"/>
  <Dimension k="1" name="temperature"/>
</NineML>
