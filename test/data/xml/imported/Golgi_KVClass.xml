<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <Dynamics name="Golgi_KVClass">
    <AnalogSendPort dimension="currentDensity" name="ik"/>
    <AnalogReceivePort dimension="temperature" name="celsius"/>
    <AnalogReceivePort dimension="voltage" name="ek"/>
    <AnalogReceivePort dimension="voltage" name="v"/>
    <Parameter dimension="voltage" name="V0beta_n"/>
    <Parameter dimension="conductanceDensity" name="gkbar"/>
    <Parameter dimension="voltage" name="Kbeta_n"/>
    <Parameter dimension="voltage" name="V0alpha_n"/>
    <Parameter dimension="dimensionless" name="Q10"/>
    <Parameter dimension="dimensionless" name="Aalpha_n"/>
    <Parameter dimension="per_time" name="Abeta_n"/>
    <Parameter dimension="voltage" name="Kalpha_n"/>
    
      <Regime name="states">
        <TimeDerivative variable="n">
          <MathInline>(n_inf - n)/tau_n</MathInline>
        </TimeDerivative>
      </Regime>
      <Alias name="tau_n">
        <MathInline>1/(a_n + b_n)</MathInline>
      </Alias>
      <Alias name="b_n">
        <MathInline>bet_n_v</MathInline>
      </Alias>
      <Alias name="alp_n_v">
        <MathInline>tcorr_v*Aalpha_n*linoid_v__m__V0alpha_n_Kalpha_n</MathInline>
      </Alias>
      <Alias name="g">
        <MathInline>gkbar*n*n*n*n</MathInline>
      </Alias>
      <Alias name="n_inf">
        <MathInline>a_n/(a_n + b_n)</MathInline>
      </Alias>
      <Alias name="alpha_n">
        <MathInline>alp_n_v</MathInline>
      </Alias>
      <Alias name="ik">
        <MathInline>g*(v - ek)</MathInline>
      </Alias>
      <Alias name="bet_n_v">
        <MathInline>tcorr_v*Abeta_n*exp((v-V0beta_n)/Kbeta_n)</MathInline>
      </Alias>
      <Alias name="a_n">
        <MathInline>alp_n_v</MathInline>
      </Alias>
      <Alias name="tcorr_v">
        <MathInline>Q10^((celsius-6.3)/10)</MathInline>
      </Alias>
      <Alias name="beta_n">
        <MathInline>bet_n_v</MathInline>
      </Alias>
      <Piecewise name="linoid_v__m__V0alpha_n_Kalpha_n">
        <Piece>
          <MathInline>Kalpha_n*(1 - (v-V0alpha_n)/Kalpha_n/2)</MathInline>
          <Condition>
            <MathInline>abs((v-V0alpha_n)/Kalpha_n) &lt; 1e-6</MathInline>
          </Condition>
        </Piece>
        <Otherwise>
          <MathInline>(v-V0alpha_n)/(exp((v-V0alpha_n)/Kalpha_n) - 1)</MathInline>
        </Otherwise>
      </Piecewise>
      <StateVariable dimension="dimensionless" name="n"/>
    </Dynamics>
  <Dimension i="2" t="3" m="-1" l="-4" name="conductanceDensity"/>
  <Dimension name="per_time" t="-1"/>
  <Dimension i="-1" t="-3" m="1" l="2" name="voltage"/>
  <Dimension i="1" name="currentDensity" l="-2"/>
  <Dimension name="dimensionless"/>
  <Dimension k="1" name="temperature"/>
</NineML>
