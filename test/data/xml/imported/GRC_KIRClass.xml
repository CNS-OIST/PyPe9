<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <Dimension name="per_time" t="-1"/>
  <Dimension i="2" t="3" m="-1" l="-4" name="conductanceDensity"/>
  <Dimension i="1" name="currentDensity" l="-2"/>
  <Dimension i="-1" t="-3" m="1" l="2" name="voltage"/>
  <Dynamics name="GRC_KIRClass">
    <AnalogSendPort dimension="currentDensity" name="ik"/>
    <AnalogReceivePort dimension="voltage" name="v"/>
    <Parameter dimension="voltage" name="ek"/>
    <Parameter dimension="voltage" name="Kbeta_d"/>
    <Parameter dimension="conductanceDensity" name="gkbar"/>
    <Parameter dimension="voltage" name="V0beta_d"/>
    <Parameter dimension="per_time" name="Abeta_d"/>
    <Parameter dimension="per_time" name="Aalpha_d"/>
    <Parameter dimension="temperature" name="celsius"/>
    <Parameter dimension="voltage" name="Kalpha_d"/>
    <Parameter dimension="voltage" name="V0alpha_d"/>
    
      <Regime name="states">
        <TimeDerivative variable="d">
          <MathInline>(d_inf - d)/tau_d</MathInline>
        </TimeDerivative>
      </Regime>
      <Alias name="bet_d_v">
        <MathInline>Q10_v*Abeta_d*exp((v-V0beta_d)/Kbeta_d)</MathInline>
      </Alias>
      <Alias name="alpha_d">
        <MathInline>alp_d_v</MathInline>
      </Alias>
      <Alias name="Q10_v">
        <MathInline>3^((celsius-20)/10)</MathInline>
      </Alias>
      <Alias name="g">
        <MathInline>gkbar*d</MathInline>
      </Alias>
      <Alias name="tau_d">
        <MathInline>1/(a_d + b_d)</MathInline>
      </Alias>
      <Alias name="b_d">
        <MathInline>bet_d_v</MathInline>
      </Alias>
      <Alias name="alp_d_v">
        <MathInline>Q10_v*Aalpha_d*exp((v-V0alpha_d)/Kalpha_d)</MathInline>
      </Alias>
      <Alias name="ik">
        <MathInline>g*(v - ek)</MathInline>
      </Alias>
      <Alias name="a_d">
        <MathInline>alp_d_v</MathInline>
      </Alias>
      <Alias name="beta_d">
        <MathInline>bet_d_v</MathInline>
      </Alias>
      <Alias name="d_inf">
        <MathInline>a_d/(a_d + b_d)</MathInline>
      </Alias>
      <StateVariable dimension="dimensionless" name="d"/>
    </Dynamics>
  <Dimension name="dimensionless"/>
  <Dimension k="1" name="temperature"/>
</NineML>
