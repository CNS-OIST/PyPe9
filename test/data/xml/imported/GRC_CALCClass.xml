<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <Unit symbol="C" dimension="charge" power="0"/>
  <Dimension name="per_time" t="-1"/>
  <Dimension i="1" name="charge" t="1"/>
  <Dynamics name="GRC_CALCClass">
    <AnalogSendPort dimension="concentration" name="cai"/>
    <AnalogReceivePort dimension="currentDensity" name="ica"/>
    <AnalogReceivePort dimension="concentration" name="cao"/>
    <Parameter dimension="per_time" name="beta"/>
    <Parameter dimension="length" name="d"/>
    <Parameter dimension="concentration" name="cai0"/>
    
      <Regime name="conc">
        <TimeDerivative variable="cai">
          <MathInline>-ica/(2*F*d)*(1e4) - beta*(cai-cai0)</MathInline>
        </TimeDerivative>
      </Regime>
      <Constant units="C" name="F" value="96485.3365"/>
      <StateVariable dimension="dimensionless" name="cai"/>
    </Dynamics>
  <Dimension name="length" l="1"/>
  <Dimension i="1" name="currentDensity" l="-2"/>
  <Dimension name="dimensionless"/>
  <Dimension l="-3" name="concentration" n="1"/>
</NineML>
