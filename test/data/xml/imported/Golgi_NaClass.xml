<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <Dimension name="per_time" t="-1"/>
  <Dimension i="2" t="3" m="-1" l="-4" name="conductanceDensity"/>
  <Dynamics name="Golgi_NaClass">
    <AnalogSendPort dimension="currentDensity" name="ina"/>
    <AnalogReceivePort dimension="voltage" name="ena"/>
    <AnalogReceivePort dimension="temperature" name="celsius"/>
    <AnalogReceivePort dimension="voltage" name="v"/>
    <Parameter dimension="voltage" name="V0beta_h"/>
    <Parameter dimension="voltage" name="V0beta_m"/>
    <Parameter dimension="voltage" name="Kalpha_h"/>
    <Parameter dimension="voltage" name="Kbeta_h"/>
    <Parameter dimension="voltage" name="Kbeta_m"/>
    <Parameter dimension="per_time" name="Abeta_h"/>
    <Parameter dimension="conductanceDensity" name="gnabar"/>
    <Parameter dimension="voltage" name="V0alpha_h"/>
    <Parameter dimension="dimensionless" name="Q10"/>
    <Parameter dimension="voltage" name="V0alpha_m"/>
    <Parameter dimension="per_time" name="Abeta_m"/>
    <Parameter dimension="dimensionless" name="Aalpha_m"/>
    <Parameter dimension="voltage" name="Kalpha_m"/>
    <Parameter dimension="per_time" name="Aalpha_h"/>
    
      <Regime name="states">
        <TimeDerivative variable="h">
          <MathInline>(h_inf - h)/tau_h</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="m">
          <MathInline>(m_inf - m)/tau_m</MathInline>
        </TimeDerivative>
      </Regime>
      <Alias name="b_h">
        <MathInline>bet_h_v</MathInline>
      </Alias>
      <Alias name="tau_m">
        <MathInline>1/(a_m + b_m)</MathInline>
      </Alias>
      <Alias name="m_inf">
        <MathInline>a_m/(a_m + b_m)</MathInline>
      </Alias>
      <Alias name="b_m">
        <MathInline>bet_m_v</MathInline>
      </Alias>
      <Alias name="tau_h">
        <MathInline>1/(a_h + b_h)</MathInline>
      </Alias>
      <Alias name="alp_m_v">
        <MathInline>tcorr_v*Aalpha_m*linoid_v__m__V0alpha_m_Kalpha_m</MathInline>
      </Alias>
      <Alias name="beta_m">
        <MathInline>bet_m_v</MathInline>
      </Alias>
      <Alias name="alpha_m">
        <MathInline>alp_m_v</MathInline>
      </Alias>
      <Alias name="h_inf">
        <MathInline>a_h/(a_h + b_h)</MathInline>
      </Alias>
      <Alias name="alpha_h">
        <MathInline>alp_h_v</MathInline>
      </Alias>
      <Alias name="bet_m_v">
        <MathInline>tcorr_v*Abeta_m*exp((v-V0beta_m)/Kbeta_m)</MathInline>
      </Alias>
      <Alias name="g">
        <MathInline>gnabar*m*m*m*h</MathInline>
      </Alias>
      <Alias name="bet_h_v">
        <MathInline>tcorr_v*Abeta_h/(1+exp((v-V0beta_h)/Kbeta_h))</MathInline>
      </Alias>
      <Alias name="beta_h">
        <MathInline>bet_h_v</MathInline>
      </Alias>
      <Alias name="a_m">
        <MathInline>alp_m_v</MathInline>
      </Alias>
      <Alias name="alp_h_v">
        <MathInline>tcorr_v*Aalpha_h*exp((v-V0alpha_h)/Kalpha_h)</MathInline>
      </Alias>
      <Alias name="ina">
        <MathInline>g*(v - ena)</MathInline>
      </Alias>
      <Alias name="tcorr_v">
        <MathInline>Q10^((celsius-20)/10)</MathInline>
      </Alias>
      <Alias name="a_h">
        <MathInline>alp_h_v</MathInline>
      </Alias>
      <Piecewise name="linoid_v__m__V0alpha_m_Kalpha_m">
        <Piece>
          <MathInline>Kalpha_m*(1 - (v-V0alpha_m)/Kalpha_m/2)</MathInline>
          <Condition>
            <MathInline>abs((v-V0alpha_m)/Kalpha_m) &lt; 1e-6</MathInline>
          </Condition>
        </Piece>
        <Otherwise>
          <MathInline>(v-V0alpha_m)/(1 - exp((v-V0alpha_m)/Kalpha_m))</MathInline>
        </Otherwise>
      </Piecewise>
      <StateVariable dimension="dimensionless" name="h"/>
      <StateVariable dimension="dimensionless" name="m"/>
    </Dynamics>
  <Dimension i="-1" t="-3" m="1" l="2" name="voltage"/>
  <Dimension i="1" name="currentDensity" l="-2"/>
  <Dimension name="dimensionless"/>
  <Dimension k="1" name="temperature"/>
</NineML>
