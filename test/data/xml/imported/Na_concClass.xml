<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <Unit symbol="C" dimension="charge" power="0"/>
  <Dimension name="per_time" t="-1"/>
  <Dimension i="1" name="currentDensity" l="-2"/>
  <Dimension i="1" name="charge" t="1"/>
  <Dimension name="length" l="1"/>
  <Dynamics name="na_concClass">
    <AnalogSendPort dimension="concentration" name="nai"/>
    <AnalogSendPort dimension="concentration" name="nao"/>
    <AnalogReceivePort dimension="currentDensity" name="ina"/>
    <Parameter dimension="per_time" name="beta"/>
    <Parameter dimension="concentration" name="nai0"/>
    <Parameter dimension="length" name="d"/>
    <Parameter dimension="concentration" name="nao0"/>
    
      <Regime name="conc">
        <TimeDerivative variable="nai">
          <MathInline>-(ina)/(2*FARADAY*d)*(1e4) - beta*(nai-nai0)</MathInline>
        </TimeDerivative>
        <TimeDerivative variable="nao">
          <MathInline>(ina)/(2*FARADAY*d)*(1e4) - beta*(nao-nao0)</MathInline>
        </TimeDerivative>
      </Regime>
      <Constant units="C" name="FARADAY">96485.3365</Constant>
      <StateVariable dimension="dimensionless" name="nai"/>
      <StateVariable dimension="dimensionless" name="nao"/>
    </Dynamics>
  <Dimension name="dimensionless"/>
  <Dimension l="-3" name="concentration" n="1"/>
</NineML>
