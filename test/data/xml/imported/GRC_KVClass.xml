<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<nineml:NineML xmlns:nineml="http://nineml.net/9ML/1.0">
  <nineml:Dimension name="per_time" t="-1"/>
  <nineml:Dimension i="2" t="3" m="-1" l="-4" name="conductanceDensity"/>
  <nineml:Dimension i="-1" t="-3" m="1" l="2" name="voltage"/>
  <nineml:Dimension i="1" name="currentDensity" l="-2"/>
  <nineml:ComponentClass name="GRC_KVClass">
    <nineml:AnalogSendPort dimension="currentDensity" name="ik"/>
    <nineml:AnalogReceivePort dimension="voltage" name="v"/>
    <nineml:Parameter dimension="voltage" name="ek"/>
    <nineml:Parameter dimension="voltage" name="V0beta_n"/>
    <nineml:Parameter dimension="conductanceDensity" name="gkbar"/>
    <nineml:Parameter dimension="voltage" name="Kbeta_n"/>
    <nineml:Parameter dimension="temperature" name="celsius"/>
    <nineml:Parameter dimension="voltage" name="V0alpha_n"/>
    <nineml:Parameter dimension="dimensionless" name="Aalpha_n"/>
    <nineml:Parameter dimension="per_time" name="Abeta_n"/>
    <nineml:Parameter dimension="voltage" name="Kalpha_n"/>
    <nineml:Dynamics>
      <nineml:Regime name="states">
        <nineml:TimeDerivative variable="n">
          <nineml:MathInline>(n_inf - n)/tau_n</nineml:MathInline>
        </nineml:TimeDerivative>
      </nineml:Regime>
      <nineml:Alias name="tau_n">
        <nineml:MathInline>1/(a_n + b_n)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="Q10_v">
        <nineml:MathInline>3^((celsius-6.3)/10)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="b_n">
        <nineml:MathInline>bet_n_v</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="alp_n_v">
        <nineml:MathInline>Q10_v*Aalpha_n*linoid_v__m__V0alpha_n_Kalpha_n</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="n_inf">
        <nineml:MathInline>a_n/(a_n + b_n)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="alpha_n">
        <nineml:MathInline>alp_n_v</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="g">
        <nineml:MathInline>gkbar*n*n*n*n</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="ik">
        <nineml:MathInline>g*(v - ek)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="bet_n_v">
        <nineml:MathInline>Q10_v*Abeta_n*exp((v-V0beta_n)/Kbeta_n)</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="a_n">
        <nineml:MathInline>alp_n_v</nineml:MathInline>
      </nineml:Alias>
      <nineml:Alias name="beta_n">
        <nineml:MathInline>bet_n_v</nineml:MathInline>
      </nineml:Alias>
      <nineml:Piecewise name="linoid_v__m__V0alpha_n_Kalpha_n">
        <nineml:Piece>
          <nineml:MathInline>Kalpha_n*(1 - (v-V0alpha_n)/Kalpha_n/2)</nineml:MathInline>
          <nineml:Condition>
            <nineml:MathInline>abs((v-V0alpha_n)/Kalpha_n) &lt; 1e-6</nineml:MathInline>
          </nineml:Condition>
        </nineml:Piece>
        <nineml:Otherwise>
          <nineml:MathInline>(v-V0alpha_n)/(exp((v-V0alpha_n)/Kalpha_n) - 1)</nineml:MathInline>
        </nineml:Otherwise>
      </nineml:Piecewise>
      <nineml:StateVariable dimension="dimensionless" name="n"/>
    </nineml:Dynamics>
  </nineml:ComponentClass>
  <nineml:Dimension name="dimensionless"/>
  <nineml:Dimension k="1" name="temperature"/>
</nineml:NineML>
