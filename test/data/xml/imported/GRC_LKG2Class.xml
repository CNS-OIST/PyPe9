<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <Dynamics name="GRC_LKG2Class">
    <AnalogSendPort dimension="currentDensity" name="il"/>
    <AnalogReceivePort dimension="voltage" name="v"/>
    <Parameter dimension="conductanceDensity" name="ggaba"/>
    <Parameter dimension="voltage" name="egaba"/>
    
      <Alias name="i">
        <MathInline>il</MathInline>
      </Alias>
      <Alias name="il">
        <MathInline>ggaba*(v - egaba)</MathInline>
      </Alias>
    </Dynamics>
  <Dimension i="-1" t="-3" m="1" l="2" name="voltage"/>
  <Dimension i="2" t="3" m="-1" l="-4" name="conductanceDensity"/>
  <Dimension i="1" name="currentDensity" l="-2"/>
</NineML>
