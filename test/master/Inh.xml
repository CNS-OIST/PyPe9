                <AnalogSendPort dimension="current" name="a"/>
                <AnalogSendPort dimension="current" name="b"/>
                <AnalogSendPort dimension="current" name="i_synaptic"/>
                <Dynamics>
                  <StateVariable dimension="current" name="a"/>
                  <StateVariable dimension="current" name="b"/>
                  <Regime name="sole">
                    <TimeDerivative variable="a">
                      <MathInline>(-a + b)/tau</MathInline>
                    </TimeDerivative>
                    <TimeDerivative variable="b">
                      <MathInline>-b/tau</MathInline>
                    </TimeDerivative>
                    <OnEvent target_regime="sole" port="input_spike">
                      <StateAssignment variable="b">
                        <MathInline>b + weight</MathInline>
                      </StateAssignment>
                    </OnEvent>
                  </Regime>
                  <Alias name="i_synaptic">
                    <MathInline>a</MathInline>
                  </Alias>
                </Dynamics>
              </ComponentClass>
            </SubDynamics>
            <SubDynamics name="pls">
              <ComponentClass name="Static">
                <Parameter dimension="current" name="weight"/>
                <AnalogSendPort dimension="current" name="fixed_weight"/>
                <Dynamics>
                  <Regime name="sole"/>
                  <Alias name="fixed_weight">
                    <MathInline>weight</MathInline>
                  </Alias>
                </Dynamics>
              </ComponentClass>
            </SubDynamics>
            <AnalogSendPortExposure sub_component="psr" port="i_synaptic" name="i_synaptic__psr"/>
            <EventReceivePortExposure sub_component="psr" port="input_spike" name="input_spike__psr"/>
            <AnalogPortConnection sender_name="pls" receiver_name="psr" send_port="fixed_weight" receive_port="weight"/>
